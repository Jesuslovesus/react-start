{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [
    "html",
    "vue"
  ],
  "globals": {
    "JSEncrypt": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-empty": 2,
    "no-multi-spaces": 2,
    "no-mixed-spaces-and-tabs": [
      2,
      "smart-tabs"
    ],
    "no-trailing-spaces": 2,
    "padded-blocks": [
      2,
      "never"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "no-console": 2,
    "no-extra-semi": 2,
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "space-before-function-paren": [
      2,
      {
        "anonymous": "never",
        "named": "never"
      }
    ],
    "newline-after-var": 0,
    "no-inline-comments": 2,
    "no-multiple-empty-lines": [
      2,
      {
        "max": 2
      }
    ],
    "semi": [
      2,
      "never"
    ],
    "no-var": 2,
    "no-redeclare": 1
  }
}